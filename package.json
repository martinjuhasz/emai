{
  "name": "emai-client",
  "version": "0.0.1",
  "description": "frontend utilizing emai backend server",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint ./client",
    "compile": "webpack && uglifyjs ./static/assets/app.js -c -m -o ./static/assets/app.min.js --source-map ./static/assets/app.min.js.map",
    "start": "npm run dev",
    "dev": "node --harmony build/webpack-dev-server",
    "dev:client": "webpack-dev-server --display-error-details --progress --colors --port 8081 --host 0.0.0.0",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "echo \"Oops: no frontend tests yet :(\" && exit 1",
    "test:dev": "npm run test -- --watch",
    "deploy": "npm run lint && npm run test && NODE_ENV=production npm run compile && surge -p dist -d sir.surge.sh"
  },
  "author": "Martin Juhasz",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "chart.js": "^2.2.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.2.2",
    "eslint-plugin-react": "^6.0.0",
    "html-webpack-plugin": "^2.22.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.14.1",
    "normalizr": "^2.2.1",
    "react": "^15.2.1",
    "react-bootstrap": "^0.30.5",
    "react-chartjs": "git://github.com/reactjs/react-chartjs.git#chartjs-v2",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-bootstrap": "^0.23.3",
    "react-select": "^1.0.0-rc.2",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "uglify-js": "^2.7.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
